{
  "private": true,
  "name": "fta-tiga-demo",
  "description": "fta-tiga-demo",
  "author": "基础架构组",
  "version": "7.57.0",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "microName": "fta-tiga",
  "techType": "react",
  "scripts": {
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "uploadAssets": "dumi update",
    "prestart": "node ./scripts/hooks/prestart.js",
    "postinstall": "lerna run build",
    "start": "cross-env DEPLOY_ENV=dev dumi dev",
    "build:dev": "cross-env DEPLOY_ENV=dev npm run build:docs",
    "build:beta": "cross-env DEPLOY_ENV=beta npm run build:docs",
    "build:prod": "cross-env DEPLOY_ENV=prod npm run build:docs",
    "deploy_dev": "npm run build:dev && fta upload -r ./deploy-dev -n fta-tiga -t dev -f 1",
    "deploy_beta": "npm run build:beta && fta upload -r ./deploy-beta -n fta-tiga -t beta -f 1",
    "deploy_prod": "npm run build:prod && fta upload -r ./deploy-prod -n fta-tiga -t release -z",
    "postdeploy_prod": "node ./scripts/hooks/postdeploy.js prod",
    "build:docs": "npm run build:packages && dumi build",
    "update:techface": "cross-env DEPLOY_ENV=techface npm run build:docs",
    "build:packages": "lerna run clean && lerna run build",
    "build:packages-thresh": "lerna run clean && lerna run build:thresh",
    "build:packages-rn": "lerna run clean && lerna run build:rn",
    "build:packages-mw": "lerna run clean && lerna run build:mw",
    "build:thresh": "rm -fr dist-thresh && taro build --type thresh && ls -lah dist-thresh",
    "build:thresh-diff": "rm -fr dist-thresh && cross-env enableDiff=1 taro build --type thresh && ls -lah dist-thresh",
    "build:thresh-diff-all": "rm -fr dist-thresh && cross-env enableDiff=1 emitFullPackage=1 taro build --type thresh && ls -lah dist-thresh",
    "build:rn": "taro build --type arn",
    "build:mw": "taro build --type mw",
    "build:add_new_packages": "yarn && npm run build:packages",
    "bootstrap": "npm-run-all clear-all bootstrap:lerna",
    "bootstrap:lerna": "lerna bootstrap",
    "publish": "npm run build:packages && lerna publish --force-publish",
    "publish:next": "npm run build:packages && lerna publish --dist-tag next --force-publish",
    "pub": "lerna publish --force-publish",
    "pub:no-git": "lerna publish --no-git-tag-version",
    "showchange": "lerna changed",
    "release": "npm run build && npm publish",
    "create": "node ./scripts/creat.js",
    "new": "npm run create",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,md,json}\"",
    "prettier:dts": "prettier --write \"**/*.d.ts\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:npm": "npx stylelint \"packages/components-mini-input/src/style/index.rn.scss\"",
    "start:amh-rn": "cross-env NODE_ENV=development taro build --type arn --watch --qr --port 10087",
    "start:thresh": "cross-env NODE_ENV=development taro build --type thresh --watch --port 10088",
    "start:h5": "cross-env NODE_ENV=development taro build --type h5 --watch",
    "start:mw": "cross-env NODE_ENV=development taro build --type mw --watch --port 10089",
    "start:weapp": "cross-env NODE_ENV=dev taro build --type weapp --watch",
    "start:alipay": "cross-env NODE_ENV=dev taro build --type alipay --watch",
    "start:tt": "cross-env NODE_ENV=dev taro build --type tt --watch",
    "build:amh-rn": "lerna run build:rn && rimraf dist-android && cross-env deviceType=android taro build --type arn --deviceType android --platform android --sourcemap",
    "build:amh-rn-ios": "lerna run build:rn && rimraf dist-ios && cross-env deviceType=ios taro build --type arn --deviceType ios --platform ios --sourcemap",
    "devtools": "react-devtools",
    "clear-all": "rimraf package-lock.json packages/*/node_modules packages/*/package-lock.json packages/*/yarn.lock",
    "prebuild:dev": "cross-env DEPLOY_ENV=dev node ./scripts/hooks/prebuild.js",
    "prebuild:beta": "cross-env DEPLOY_ENV=beta node ./scripts/hooks/prebuild.js",
    "prebuild:prod": "cross-env DEPLOY_ENV=prod node ./scripts/hooks/prebuild.js",
    "version:lerna": "lerna version --no-push",
    "postversion:lerna": "node ./scripts/hooks/postversion.js"
  },
